// C언어코딩 :: 문자열 입출력 gets

#include <stdio.h>

int main(void)
{
	
	   char team[10];
	   puts("-----------");
	   puts("** 문자열 입출력 **");
	   puts("-------------------");
				
				printf(" 좋아하는 야구팀은 : ");
				gets(team);
				printf(" 야구 팀은 = %s \n", team);
				puts("--------------------------");
				puts("scanf는 공백을 만나면 입력이 종료되고,");
				puts("따라서, 공백을 포함할 때는 사용 x "); 
				
			 return 0;
				 
	} 
